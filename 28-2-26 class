        " #base class\n",
        " class member:\n",
        "  def __init_(self,name,membershipID):\n",
        "    self.name=name\n",
        "    self.membershipID=membershipID\n",
        "  def display(self):\n",
        "    print(\"name:\",self.name)\n",
        "    print(\"membershipID:\",self.membershipID)\n",
        "\n",
        "#derived class-regularmember\n",
        "class regularmember(member):\n",
        "  def getborrowlimit(self):\n",
        "    return3\n",
        "\n",
        "  def display(self):\n",
        "    print(\"member type:premium member\")\n",
        "    print(\"name:\",self.name)\n",
        "    print(\"browinglimit:\",self.getborrowlimit()\"books\")\n",
        "\n",
        "#derived class-premiummember\n",
        "  class premiummember(member):\n",
        "    def getborrowlimit(self):\n",
        "      return10\n",
